case "$1" in 
   1)
     ${EXEC}/logisch1/logisch1.html
     ;;
   2)
     ${EXEC}/logisch2/logisch2.html
     ;;
   3)
     ${EXEC}/logisch3/logisch3.html
     ;;
   4)
     ${EXEC}/logisch4/logisch4.html
     ;;
   5)
     ${EXEC}/logisch5/logisch5.html
     ;;
   6)
     ${EXEC}/logisch6/logisch6.html
     ;;
   *)
     ${EXEC}/index.html
     ;;
esac



 
 
